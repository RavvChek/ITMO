SELECT Н_ЛЮДИ.ИМЯ, Н_СЕССИЯ.ЧЛВК_ИД FROM Н_ЛЮДИ NATURAL RIGHT JOIN Н_СЕССИЯ WHERE Н_ЛЮДИ.ОТЧЕСТВО = 'Георгиевич' AND Н_СЕССИЯ.ЧЛВК_ИД < 126631;
SELECT Н_ЛЮДИ.ФАМИЛИЯ, Н_ВЕДОМОСТИ.ДАТА, Н_СЕССИЯ.ИД FROM Н_ЛЮДИ NATURAL LEFT JOIN Н_ВЕДОМОСТИ NATURAL LEFT JOIN Н_СЕССИЯ WHERE Н_ЛЮДИ.ФАМИЛИЯ = 'Ёлкин' AND Н_ВЕДОМОСТИ.ЧЛВК_ИД < 117219 AND Н_СЕССИЯ.ИД < 27640;